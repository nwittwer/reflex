{
  "name": "reflex",
  "productName": "Reflex",
  "description": "Make responsive websites without the guesswork.",
  "version": "0.6.6-beta.2",
  "author": "Nick Wittwer",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "start": "node .electron-vue/dev-runner.js",
    "build": "node .electron-vue/build.js && electron-builder -m",
    "build:dir": "node .electron-vue/build.js && electron-builder -m --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint 'src/**/*.{js,vue}' --fix",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "electron-builder install-app-deps",
    "release": "gulp release",
    "oldRelease": "gulp build",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nwittwer/reflex.git"
  },
  "private": true,
  "build": {
    "appId": "com.reflex.app",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/renderer/$1"
    }
  },
  "dependencies": {
    "@simonwep/selection-js": "^1.2.0",
    "babel-preset-minify": "^0.5.1",
    "browser-sync": "^2.26.7",
    "electron-debug": "^3.0.1",
    "electron-is-dev": "^1.1.0",
    "electron-log": "^3.0.7",
    "electron-updater": "^4.1.2",
    "is-electron": "^2.2.0",
    "lodash.debounce": "^4.0.8",
    "uuid": "^3.3.3",
    "vue": "^2.6.10",
    "vue-electron": "^1.0.6",
    "vue-router": "^3.1.2",
    "vuedraggable": "^2.23.0",
    "vuex": "^3.1.1",
    "vuex-electron": "^1.0.0",
    "vuex-persist": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@vue/cli": "^3.11.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-require-context-hook": "^1.0.0",
    "browser-sync-webpack-plugin": "^2.2.2",
    "cfonts": "^2.4.5",
    "chalk": "^2.4.2",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^5.1.6",
    "css-loader": "^3.2.0",
    "del": "^5.1.0",
    "devtron": "^1.4.0",
    "electron": "^6.0.4",
    "electron-builder": "^21.2.0",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^5.2.3",
    "file-loader": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-hub": "^4.2.0",
    "gulp-notify": "^3.2.0",
    "gulp-replace": "^1.0.0",
    "html-webpack-plugin": "^4.0.0-beta.8",
    "jest": "^24.9.0",
    "jest-vue-preprocessor": "^1.5.0",
    "mini-css-extract-plugin": "^0.8.0",
    "moment": "^2.24.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.12.0",
    "opn": "^6.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "url-loader": "^2.1.0",
    "vue-eslint-parser": "^6.0.4",
    "vue-html-loader": "^1.2.4",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.7.1",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2"
  }
}
