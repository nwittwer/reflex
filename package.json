{
  "name": "reflex",
  "productName": "Reflex",
  "description": "Make responsive websites without the guesswork.",
  "version": "0.7.0-beta.1",
  "author": "Nick Wittwer",
  "license": "MIT",
  "main": "./dist/main/index.js",
  "scripts": {
    "dev": "node .electron-nuxt/check-engines.js && cross-env-shell NODE_ENV=development node .electron-nuxt/index.js",
    "build": "node .electron-nuxt/check-engines.js && cross-env-shell NODE_ENV=production node .electron-nuxt/index.js",
    "release": "cross-env-shell RELEASE=true yarn run build",
    "test": "jest",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "preinstall": "node .electron-nuxt/check-engines.js",
    "postinstall": "electron-builder install-app-deps",
    "storybook": "start-storybook"
  },
  "keywords": [],
  "homepage": "https://reflexapp.nickwittwer.com",
  "engines": {
    "node": ">=8.10.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/renderer/$1"
    }
  },
  "dependencies": {
    "browser-sync": "^2.26.7",
    "prettier": "^2.0.4"
  },
  "devDependencies": {
    "@babel/runtime-corejs2": "7.9.2",
    "@nuxtjs/eslint-config": "^2.0.2",
    "@nuxtjs/eslint-module": "^1.1.0",
    "@panzoom/panzoom": "^4.1.0",
    "@simonwep/selection-js": "^1.5.0",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-notes": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/vue": "^5.3.18",
    "@vue/test-utils": "^1.0.0-beta.32",
    "@xpda-dev/core": "^0.1.5",
    "@xpda-dev/electron-builder": "^0.1.1",
    "@xpda-dev/electron-launcher": "^0.1.1",
    "@xpda-dev/webpack-step": "^0.1.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-preset-vue": "^2.0.2",
    "browser-env": "3.3.0",
    "core-js": "2.6.5",
    "cross-env": "7.0.2",
    "deepmerge": "4.2.2",
    "devtron": "^1.4.0",
    "dotenv": "^8.2.0",
    "electron": "8.2",
    "electron-builder": "^22.3.2",
    "electron-debug": "3.0.1",
    "electron-is-dev": "1.2",
    "electron-log": "4.1",
    "electron-notarize": "0.3",
    "electron-updater": "4.2.5",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-nuxt": "^0.5.2",
    "eslint-plugin-prettier": "^3.1.3",
    "gulp": "^4.0.2",
    "gulp-hub": "^4.2.0",
    "gulp-notify": "^3.2.0",
    "gulp-replace": "^1.0.0",
    "is-electron": "^2.2.0",
    "jest": "25",
    "jest-vue-preprocessor": "^1.7.0",
    "lodash.debounce": "^4.0.8",
    "moment": "^2.24.0",
    "node-sass": "4.13.1",
    "nuxt": "2.12.2",
    "sass-loader": "8.0.2",
    "uuid": "7.0.3",
    "vue-devtools": "5.1.3",
    "vue-jest": "^3.0.5",
    "vuedraggable": "^2.23.2",
    "vuex-persist": "^2.1.1",
    "webpack": "4.42.1",
    "webpack-node-externals": "1.7.2"
  }
}
