{
  "name": "reflex",
  "productName": "Reflex",
  "description": "Make responsive websites without the guesswork.",
  "version": "0.7.0-beta.5",
  "author": "Nick Wittwer",
  "main": "./dist/main/index.js",
  "homepage": "https://reflexapp.nickwittwer.com",
  "private": "true",
  "scripts": {
    "dev": "npx cross-env PLAYWRIGHT_BROWSERS_PATH=0 node .electron-nuxt/dev.js",
    "build": "yarn clean:local-browsers && npx cross-env PLAYWRIGHT_BROWSERS_PATH=0 FULLBUILD=1 node .electron-nuxt/build.js",
    "build:fast": "yarn clean:local-browsers && npx cross-env PLAYWRIGHT_BROWSERS_PATH=0 CSC_IDENTITY_AUTO_DISCOVERY=false node .electron-nuxt/build.js",
    "test": "jest",
    "lint": "node_modules/.bin/eslint --ext .js,.vue,.ts -f ./node_modules/eslint-friendly-formatter ./src",
    "lint:fix": "yarn run lint -- --fix",
    "preinstall": "npx cross-env PLAYWRIGHT_SKIP_BROWSER_DOWNLOAD=1 node .electron-nuxt/check-engines.js",
    "postinstall": "electron-builder install-app-deps",
    "storybook": "start-storybook",
    "debug:playwright": "npx cross-env DEBUG=pw:browser* yarn run dev",
    "debug:build:mac": "./build/mac/Reflex.app/Contents/MacOS/Reflex --args --remote-debugging-port=8315 & open -a \"Google Chrome\" http://localhost:8315",
    "clean:local-browsers": "npx rimraf ./node_modules/reflex-browser-installer/dist/.local-browsers/*"
  },
  "engines": {
    "node": ">=12.13.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/renderer/$1",
      "^~/(.*)$": "<rootDir>/src/renderer/$1"
    }
  },
  "dependencies": {
    "playwright-core": "^1.9.2"
  },
  "devDependencies": {
    "@babel/runtime-corejs2": "^7.13.0",
    "@electron/remote": "^1.0.4",
    "@nuxt/types": "^2.15.4",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config": "^6.0.0",
    "@nuxtjs/eslint-config-typescript": "6.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@panzoom/panzoom": "^4.1.0",
    "@simonwep/selection-js": "^2.0.0",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-knobs": "^6.1.11",
    "@storybook/addon-notes": "^5.3.18",
    "@storybook/addons": "^6.1.11",
    "@storybook/vue": "^6.1.11",
    "@vue/composition-api": "^1.0.0-beta.21",
    "@vue/test-utils": "^1.0.0-beta.32",
    "@xpda-dev/core": "^0.1.5",
    "@xpda-dev/electron-builder": "^0.2.1",
    "@xpda-dev/electron-launcher": "^0.3.0",
    "@xpda-dev/webpack-step": "^0.1.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-preset-vue": "^2.0.2",
    "browser-env": "3.3.0",
    "comlink": "^4.3.0",
    "comlink-loader": "^2.0.0",
    "core-js": "^3.9.0",
    "cross-env": "^7.0.3",
    "css-loader": "5.0.0",
    "deepmerge": "4.2.2",
    "devtron": "^1.4.0",
    "dotenv": "^8.2.0",
    "electron": "^12.0.1",
    "electron-builder": "^22.9.1",
    "electron-debug": "3.2.0",
    "electron-devtools-installer": "3.1.1",
    "electron-is-dev": "^2.0",
    "electron-log": "^4.3.0",
    "electron-notarize": "1.0.0",
    "electron-updater": "^4.3.5",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "is-electron": "^2.2.0",
    "jest": "26.6.3",
    "jest-vue-preprocessor": "^1.7.0",
    "lodash.debounce": "^4.0.8",
    "moment": "^2.24.0",
    "native-ext-loader": "2.3.0",
    "node-sass": "4.14.1",
    "nuxt": ">2.14.1",
    "postcss": "8.1.10",
    "postcss-import": "13.0.0",
    "postcss-loader": "4.1.0",
    "postcss-url": "10.1.1",
    "prettier": "^2.0.4",
    "reflex-browser-installer": "^1.0.0",
    "sass-loader": "10.0.2",
    "uuid": "8.3.2",
    "vue-devtools": "5.1.4",
    "vue-feature-flipping": "^3.0.0",
    "vue-jest": "^3.0.5",
    "vuedraggable": "^2.23.2",
    "vuex-persist": "^3.1.0",
    "webpack": "^4.45.0",
    "webpack-node-externals": "^2.5.0"
  }
}
